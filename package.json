{
  "name": "testingspace",
  "author": "Ryan Dawes",
  "dependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/preset-typescript": "latest",
    "gh-pages": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "css-build": "sass --no-source-map _sass/style.scss css/style.css",
    "js-build": "babel --extensions '.tsx' _javascript --out-dir lib",
    "build": "npm-run-all --parallel css-build js-build",
    "copy": "rm -rf dist && mkdir dist && cp CNAME css/style.css lib/main.js *.html ./dist && cp -r assets/ ./dist",
    "pages": "gh-pages -d ./dist"
  },
  "devDependencies": {
    "npm": "^8.13.2"
  }
}
