{
  "name": "testingspace",
  "author": "Ryan Dawes",
  "private": true,
  "dependencies": {
    "typescript": "latest",
    "sass": "latest"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "npm": "^8.13.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "process": "^0.11.10"
  },
  "scripts": {
    "dev": "parcel src/*.html src/about/*.html src/contact/*.html",
    "build": "parcel build src/*.html src/about/*.html src/contact/*.html",
    "clean": "rm -rf dist/",
    "copy": "cp CNAME dist/",
    "pages": "gh-pages -d dist/",
    "deploy": "npm-run-all --sequential clean build copy pages"
  }
}
